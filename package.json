{
  "name": "breadcrumb",
  "description": "WIP - nothing to see here",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    "import": "./index.js"
  },
  "author": "Bret Comnes <bcomnes@gmail.com> (https://bret.io)",
  "bugs": {
    "url": "https://github.com/bcomnes/breadcrumb/issues"
  },
  "dependencies": {
    "@nearform/sql": "^1.5.0",
    "fastify": "^3.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-autoload": "^3.0.2",
    "fastify-cli": "^2.5.1",
    "fastify-postgres": "^3.3.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "auto-changelog": "^2.0.0",
    "dependency-check": "^4.1.0",
    "gh-release": "^4.0.5-beta.0",
    "npm-run-all2": "^5.0.0",
    "postgrator": "^4.1.1",
    "postgrator-cli": "^4.0.0",
    "standard": "^16.0.0",
    "tap": "^14.10.8"
  },
  "homepage": "https://github.com/bcomnes/breadcrumb",
  "keywords": [],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcomnes/breadcrumb.git"
  },
  "scripts": {
    "prepublishOnly": "git push --follow-tags && gh-releas -y",
    "test": "run-s test:*",
    "test:deps": "dependency-check package.json --no-dev --no-peer",
    "test:standard": "standard",
    "test:tap": "tap test/**/*.test.js --no-esm --jobs=1",
    "version": "run-s version:*",
    "version:changelog": "auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:'",
    "version:git": "git add CHANGELOG.md",
    "migrate": "postgrator",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js"
  },
  "standard": {
    "ignore": [
      "dist",
      "cjs"
    ]
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/bcomnes"
  }
}
